{
  "name": "vs-time-machine",
  "version": "0.1.0",
  "private": true,
  "productName": "VS Time Machine",
  "dependencies": {
    "@craco/craco": "~5.6.0",
    "@electron/remote": "~2.0.1",
    "@emotion/styled": "~11.6.0",
    "@mui/icons-material": "5.2.4",
    "@mui/material": "~5.2.4",
    "@mui/styles": "~5.2.3",
    "@mui/x-data-grid": "~5.2.1",
    "@mui/x-data-grid-generator": "~5.2.1",
    "@mui/x-data-grid-pro": "~5.2.1",
    "@testing-library/jest-dom": "~5.16.1",
    "@testing-library/react": "~12.1.2",
    "@testing-library/user-event": "~13.5.0",
    "classnames": "~2.3.1",
    "electron-is-dev": "~2.0.0",
    "lodash": "~4.17.21",
    "luxon": "~2.2.0",
    "mobx": "~6.3.8",
    "mobx-react": "~7.2.1",
    "nedb": "~1.8.0",
    "nedb-promises": "~5.0.2",
    "node-sass": "~7.0.0",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-router-dom": "~6.1.1",
    "react-scripts": "5.0.0",
    "serialport": "~9.2.8",
    "ts-enum-util": "~4.0.2",
    "uuid": "~8.3.2",
    "web-vitals": "~2.1.2"
  },
  "devDependencies": {
    "@types/jest": "~26.0.24",
    "@types/lodash": "^4.14.178",
    "@types/luxon": "^2.0.8",
    "@types/nedb": "^1.8.12",
    "@types/node": "~12.20.37",
    "@types/react": "~17.0.37",
    "@types/react-dom": "~17.0.11",
    "@types/serialport": "^8.0.2",
    "@types/uuid": "^8.3.3",
    "concurrently": "~6.4.0",
    "craco-alias": "~3.0.1",
    "cross-env": "~7.0.3",
    "electron": "~16.0.5",
    "electron-builder": "~22.14.5",
    "electron-rebuild": "~3.2.5",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "~2.25.3",
    "eslint-plugin-prettier": "~4.0.0",
    "prettier": "~2.5.1",
    "typescript": "~4.5.4",
    "wait-on": "~6.0.0",
    "rimraf": "~3.0.2"
  },
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "start": "export BROWSER=none && craco start",
    "build": "craco build",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm run start\" \"npm run electron:start\"",
    "electron:build": "rimraf ./dist && npm run build && electron-builder -c.extraMetadata.main=electron/main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "rebuild": "electron-rebuild -f -w serialport"
  },
  "build": {
    "extends": null,
    "productName": "VS Time Machine",
    "appId": "com.vstimemachine",
    "files": [
      "dist/**/*",
      "build/**/*",
      "assets/**/*",
      "electron/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "icon": "assets/icon/AppIcon.icns",
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "assets/icon/icon_256@1x.png"
    },
    "directories": {
      "buildResources": "assets"
    },
    "buildDependenciesFromSource": true,
    "npmRebuild": false
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
